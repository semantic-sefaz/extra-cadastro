# Mapeamento de Empresa@Organization
<#TriplesMapOrganizationCadastro> a rr:TriplesMap;
    rr:logicalTable [ rr:sqlQuery """SELECT * FROM "UFC_SEM"."CAD_EMPRESA"
 """ ];
    rr:subjectMap [
        rr:template "http://www.sefaz.ma.gov.br/resource/Cadastro_SEFAZ-MA/Empresa/{ID}";
        rr:class foaf:Organization, sefazma:Empresa_Cadastro;
        ##rr:graph <http://www.sefaz.ma.gov.br/ontology/Graph/Cadastro_SEFAZ-MA> ; 
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label,  sefazma:razao_social;
        rr:objectMap [ rr:column "razao_social"; rr:datatype xsd:string ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:cnpj_raiz;
        rr:objectMap [ rr:column "cnpj_raiz"; rr:datatype xsd:string ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:cnpj;
        rr:objectMap [ rr:column "cnpj_raiz"; rr:datatype xsd:string ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:siglas;
        rr:objectMap [ rr:column "siglas"; rr:datatype xsd:string ];
    ];
    rr:predicateObjectMap [
        rr:predicate sefazma:data_incorporacao;
        rr:objectMap [ rr:column "data_incorporacao"; rr:datatype xsd:string ];
    ];
    #rr:predicateObjectMap [
    #    rr:predicate sefazma:data_modificacao;
    #    rr:objectMap [ rr:column "data_modificacao"; rr:datatype xsd:date];
    #];

    rr:predicateObjectMap [
       rr:predicate sefazma:data_constituicao;
        rr:objectMap [ rr:column "data_constituicao"; rr:datatype xsd:date ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:data_fechamento_liquido;
        rr:objectMap [ rr:column "data_fechamento_liquido"; rr:datatype xsd:date ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:data_fechamento_fiscal;
        rr:objectMap [ rr:column "data_fechamento_fiscal"; rr:datatype xsd:date ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:valor_capital_social;
        rr:objectMap [ rr:column "valor_capital_social"; rr:datatype xsd:double ];
    ];
    rr:predicateObjectMap [
        rr:predicate sefazma:tem_natureza_legal;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/DOMINIO/Natureza_Legal/{ID_natureza_legal}"; ];
    ];
    rr:predicateObjectMap [
        rr:predicate sefazma:tem_porte;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/Cadastro_SEFAZ-MA/Porte/{ID_porte}"; ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:tem_usuario;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/Cadastro_SEFAZ-MA/Usuario/{ID_usuario}"; ];
    ].


# Mapeamento tem_sociedade
<#hasPartnershipCadastro> a rr:TriplesMap;
    rr:logicalTable [ rr:sqlQuery """SELECT ID_empresa,ID_sociedade FROM "UFC_SEM"."CAD_TEM_SOCIEDADE" """ ];
    rr:subjectMap [
        rr:template "http://www.sefaz.ma.gov.br/resource/Cadastro_SEFAZ-MA/Empresa/{ID_empresa}";
        ##rr:graph <http://www.sefaz.ma.gov.br/ontology/Graph/Cadastro_SEFAZ-MA> ; 
    ];
    rr:predicateObjectMap [
        rr:predicate sefazma:tem_sociedade, sefazma:tem_sociedade_cadastro;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/Cadastro_SEFAZ-MA/Sociedade/{ID_sociedade}"; ];
    ].


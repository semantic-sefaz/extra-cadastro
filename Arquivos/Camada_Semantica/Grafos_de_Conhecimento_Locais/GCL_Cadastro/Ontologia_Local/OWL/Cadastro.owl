###  http://www.sefaz.ma.gov.br/ontology/ACTIVE
vcad:ACTIVE rdf:type owl:Class ;
            rdfs:subClassOf vcad:SEFAZ_CadastralSituation ;
            rdfs:comment "Subclasse de Situação Cadastral - Representa o Status de Situação Cadastral: ATIVO"@pt ;
            rdfs:label "ACTIVE"@en ,
                       "ATIVO"@pt .


###  http://www.sefaz.ma.gov.br/ontology/Accountant
vcad:Accountant rdf:type owl:Class ;
                rdfs:subClassOf vcad:Qualification ,
                                [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty vcad:tem_qualificacao_socio ;
                                                         owl:allValuesFrom vcad:Qualification
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty vcad:tem_qualificacao_socio ;
                                                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
                rdfs:comment "Accountant is the professional of accounting science or accounting responsible for dealing with the entire financial, economic and patrimonial area of a legal entity or individual."@en ,
                             "É o profissional da ciência contábil ou contabilidade responsável por lidar com toda a área financeira, econômica e patrimonial de uma pessoa jurídica ou pessoa física."@pt ;
                rdfs:label "Accountant"@en ,
                           "Contador"@pt .


###  http://www.sefaz.ma.gov.br/ontology/Address
vcad:Address rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty vcad:numero_telefone ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty rdfs:label ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty xml:postal-code ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty vcad:numero_telefone ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ;
             rdfs:comment "Representa o Endereço de uma determinada Empresa, Estabelecimento ou Pessoa Física."@pt ;
             rdfs:label "Address"@en ,
                        "Endereço"@pt .



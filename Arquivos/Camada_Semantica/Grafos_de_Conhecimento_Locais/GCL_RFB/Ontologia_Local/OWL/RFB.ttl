###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty vcard:email ;
                             owl:allValuesFrom xsd:anyURI
                           ] .


###  http://xmlns.com/foaf/0.1/Group
foaf:Group rdf:type owl:Class ;
           rdfs:subClassOf foaf:Agent ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty foaf:member ;
                                                    owl:someValuesFrom foaf:Organization
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty foaf:member ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass foaf:Organization
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class ;
                  rdfs:subClassOf foaf:Agent ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rfb:qualificacao_representante ;
                                    owl:someValuesFrom rfb:Qualificacao
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rfb:tem_matriz ;
                                    owl:someValuesFrom rfb:Estabelecimento
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rfb:qualificacao_representante ;
                                    owl:allValuesFrom rfb:Qualificacao
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rfb:cnpj_raiz ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rfb:razao_social ;
                                    owl:someValuesFrom xsd:string
                                  ] ;
                  owl:disjointWith foaf:Person .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:subClassOf foaf:Agent ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty foaf:name ;
                              owl:someValuesFrom xsd:string
                            ] .



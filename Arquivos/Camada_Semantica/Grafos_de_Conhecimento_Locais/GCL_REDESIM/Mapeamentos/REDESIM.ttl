        rr:objectMap [ rr:column "complemento"; rr:datatype xsd:string ];
    ];
    rr:predicateObjectMap [
        rr:predicate sefazma:cep_caixa_postal;
        rr:objectMap [ rr:column "cep_caixa_postal"; rr:datatype xsd:string ];
    ];
    rr:predicateObjectMap [
        rr:predicate vcard:postal-code;
        rr:objectMap [ rr:column "cep"; rr:datatype xsd:string ];
    ];


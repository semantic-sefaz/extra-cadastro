
# Mapeamento Empresa
<#TriplesMapOrganizationREDESIM>
    rr:logicalTable [ rr:sqlQuery """SELECT * FROM "UFC_SEM"."REDESIM_EMPRESA" """ ];
    rr:subjectMap [
        rr:template "http://www.sefaz.ma.gov.br/resource/REDESIM/Empresa/{ID}";
        rr:class foaf:Organization, sefazma:Empresa_REDESIM ;
       #rr:graph <http://www.sefaz.ma.gov.br/ontology/Graph/REDESIM> ; 
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rr:column "id"; rr:datatype xsd:string ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:razao_social;
        rr:objectMap [ rr:column "razao_social"; rr:datatype xsd:string ];
    ];
    
    rr:predicateObjectMap [
        rr:predicate sefazma:cnpj;
        rr:objectMap [ rr:column "id"; rr:datatype xsd:string ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:cnpj_raiz;
        rr:objectMap [ rr:column "cnpj_raiz"; rr:datatype xsd:string ];
    ];
    
    rr:predicateObjectMap [
        rr:predicate sefazma:nire;
        rr:objectMap [ rr:column "nire"; rr:datatype xsd:string ];
    ];
    
    rr:predicateObjectMap [
        rr:predicate sefazma:data_ultimo_arquivamento, sefazma:data_atualizacao;
        rr:objectMap [ rr:column "data_ultimo_arquivamento"; rr:datatype xsd:date];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:data_assinatura;
        rr:objectMap [ rr:column "data_assinatura"; rr:datatype xsd:date];
    ];
    
    rr:predicateObjectMap [
        rr:predicate sefazma:data_constituicao;
        rr:objectMap [ rr:column "data_constituicao"; rr:datatype xsd:date ];
    ];
    
    rr:predicateObjectMap [
        rr:predicate sefazma:valor_capital_social;
        rr:objectMap [ rr:column "capital_social"; rr:datatype xsd:double ];
    ];

    rr:predicateObjectMap [
        rr:predicate foaf:homepage;
        rr:objectMap [ rr:column "site"; rr:datatype xsd:date ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:tem_porte;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/REDESIM/Porte/{ID_porte}"; ];
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:tem_natureza_legal;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/DOMINIO/Natureza_Legal/{ID_natureza_legal}"; ];
    ].


# Mapeamento Empresa tem_Sociedade
<#TriplesMapOrganizationHasSociedadeREDESIM>
    rr:logicalTable [ rr:sqlQuery """SELECT ID_empresa,ID_sociedade FROM "UFC_SEM"."REDESIM_TEM_SOCIEDADE" """ ];
    rr:subjectMap [
        rr:template "http://www.sefaz.ma.gov.br/resource/REDESIM/Empresa/{ID_empresa}";
        #rr:graph <http://www.sefaz.ma.gov.br/ontology/Graph/REDESIM> ; 
    ];

    rr:predicateObjectMap [
        rr:predicate sefazma:tem_sociedade, sefazma:tem_sociedade_redesim;
        rr:objectMap [ rr:template "http://www.sefaz.ma.gov.br/resource/REDESIM/Sociedade/{ID_sociedade}"; ];
    ].

